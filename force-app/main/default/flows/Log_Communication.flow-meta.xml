    <formulas>
        <name>ContactRecord</name>
        <dataType>String</dataType>
        <expression>IF(
{!Opportunity}, {!Get_Opportunity.Primary_Contact__c},{!Get_Contact.Id})</expression>
    </formulas>
    <formulas>
        <name>EventSubjectSelection</name>
        <dataType>String</dataType>
        <expression>IF(
 {!Event} = &quot;Event&quot;,
  IF(
    AND(
      NOT(ISBLANK({!Event_Subject_Picklist})),
      {!Event_Subject_Picklist} &lt;&gt; &quot;Custom&quot;
    ),
  {!Event_Subject_Picklist},
 {!Event_Subject}
  ),
  &quot;&quot;
)</expression>
    </formulas>
    <formulas>
        <name>RelatedToId</name>
        <dataType>String</dataType>
        <expression>IF({!Opportunity}, {!Get_Opportunity.Id},{!Get_Contact.AccountId})</expression>
    </formulas>
    <formulas>
        <name>TaskSubjectSelection</name>
        <dataType>String</dataType>
        <expression>IF(
  {!Task} = &quot;Task&quot;,
  IF(
    AND(
      NOT(ISBLANK({!Task_Subject_Picklist})),
      {!Task_Subject_Picklist} &lt;&gt; &quot;Custom&quot;
    ),
    {!Task_Subject_Picklist},
    {!Subject}
  ),
  &quot;&quot;
)</expression>
    </formulas>
